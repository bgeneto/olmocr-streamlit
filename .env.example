# olmOCR Web Interface Configuration

# Default vLLM Server Settings
VLLM_BASE_URL=http://vllm-server:8000
VLLM_MODEL=allenai/olmOCR-7B-0725-FP8
# VLLM_API_KEY is optional - only set if your vLLM server requires authentication
VLLM_API_KEY=your_api_key_here

# Streamlit App Settings
STREAMLIT_PORT=8501
MAX_FILE_SIZE_MB=100
DEFAULT_WORKERS=10

# Processing Settings
DEFAULT_TARGET_DIM=1288
DEFAULT_APPLY_FILTER=true
DEFAULT_GUIDED_DECODING=false

# Docker Settings
VLLM_GPU_MEMORY_UTILIZATION=0.9
VLLM_MAX_MODEL_LEN=16384
VLLM_TENSOR_PARALLEL_SIZE=1
